<service>
	<id>osdndbot.exe</id>
	<name>OSDnD Bot</name>
	<description>OSDnD Discord bot service for DMTools.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>D:\Dev\DMTools\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\dev\DMTools\bot.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>OSDnD Bot wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>SATURN</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>D:\Dev\DMTools</workingdirectory>
</service>